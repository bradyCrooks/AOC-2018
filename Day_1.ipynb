    "# Part 1\n",
    "freq = 0\n",
    "with open('day1_entry.txt.txt', 'r') as file:\n",
    "    for line in file:\n",
    "        op = line[0]\n",
    "        num = int(line[1:])\n",
    "        if op == \"-\":\n",
    "            num *= -1\n",
    "        freq += num\n",
    "print(freq)"
   ]
